<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.WeModelMapper">

    <resultMap type="WeModel" id="WeModelResult">
        <result property="id"    column="id"    />
        <result property="name"    column="name"    />
        <result property="modelUrl"    column="model_url"    />
        <result property="modelWeId"    column="model_we_id"    />
        <result property="type"    column="type"    />
        <result property="delFlag"    column="del_flag"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectWeModelVo">
        select id, name, model_url, model_we_id, type, del_flag, create_by, create_time, update_by, update_time, remark from we_model
    </sql>

    <select id="selectWeModelList" parameterType="WeModel" resultMap="WeModelResult">
        <include refid="selectWeModelVo"/>
        <where>
            <if test="id != null "> and id = #{id}</if>
            <if test="name != null  and name != ''"> and name like concat('%', #{name}, '%')</if>
            <if test="modelUrl != null  and modelUrl != ''"> and model_url = #{modelUrl}</if>
            <if test="modelWeId != null  and modelWeId != ''"> and model_we_id = #{modelWeId}</if>
            <if test="type != null  and type != ''"> and type = #{type}</if>
        </where>
    </select>

    <select id="selectWeModelById" parameterType="Long" resultMap="WeModelResult">
        <include refid="selectWeModelVo"/>
        where id = #{id}
    </select>
    
    <select id="selectWeModelByWeId" parameterType="String" resultMap="WeModelResult">
        <include refid="selectWeModelVo"/>
        where model_we_id = #{modelWeId}
    </select>

    <insert id="insertWeModel" parameterType="WeModel" useGeneratedKeys="true" keyProperty="id">
        insert into we_model
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="name != null">name,</if>
            <if test="modelUrl != null">model_url,</if>
            <if test="modelWeId != null">model_we_id,</if>
            <if test="type != null">type,</if>
            <if test="delFlag != null">del_flag,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="name != null">#{name},</if>
            <if test="modelUrl != null">#{modelUrl},</if>
            <if test="modelWeId != null">#{modelWeId},</if>
            <if test="type != null">#{type},</if>
            <if test="delFlag != null">#{delFlag},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
        </trim>
    </insert>

    <update id="updateWeModel" parameterType="WeModel">
        update we_model
        <trim prefix="SET" suffixOverrides=",">
            <if test="name != null">name = #{name},</if>
            <if test="modelUrl != null">model_url = #{modelUrl},</if>
            <if test="modelWeId != null">model_we_id = #{modelWeId},</if>
            <if test="type != null">type = #{type},</if>
            <if test="delFlag != null">del_flag = #{delFlag},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteWeModelById" parameterType="Long">
        delete from we_model where id = #{id}
    </delete>

    <delete id="deleteWeModelByIds" parameterType="String">
        delete from we_model where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>